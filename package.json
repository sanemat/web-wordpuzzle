{
  "name": "web-wordpuzzle",
  "description": "word puzzle",
  "version": "0.3.0",
  "author": "Matt (Sanemat) (Murahashi Kenichi)",
  "bugs": {
    "url": "https://github.com/sanemat/web-wordpuzzle/issues"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.26.0",
    "bulma": "^0.9.2",
    "eslint": "^7.27.0",
    "prettier": "^2.3.0",
    "serve": "^11.3.2",
    "type-coverage": "^2.17.5",
    "typescript": "^4.3.2"
  },
  "homepage": "https://github.com/sanemat/web-wordpuzzle#readme",
  "license": "Apache-2.0",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
